{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "ajithnow/laravel-subscription-webhooks": "^1.1",
        "fgrosse/phpasn1": "^2.5",
        "firebase/php-jwt": "^6.11",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel-notification-channels/onesignal": "^2.5",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.4",
        "lavary/laravel-menu": "^1.8",
        "phpseclib/phpseclib": "~3.0",
        "readdle/app-store-receipt-verification": "^1.7",
        "readdle/app-store-server-api": "^3.10",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^6.0",
        "stripe/stripe-php": "^18.0",
        "symfony/mailer": "^6.0",
        "yajra/laravel-datatables-buttons": "^10.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
